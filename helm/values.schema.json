{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "advertiseDomain": {
      "type": "string"
    },
    "bbsAddress": {
      "type": "string"
    },
    "caCertificate": {
      "type": "string"
    },
    "fileServer": {
      "additionalProperties": false,
      "properties": {
        "address": {
          "type": "string"
        }
      },
      "required": [
        "address"
      ],
      "type": "object"
    },
    "global": {
      "additionalProperties": true,
      "type": "object"
    },
    "image": {
      "additionalProperties": false,
      "properties": {
        "pullPolicy": {
          "type": "string"
        },
        "repository": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "instanceIdentityCA": {
      "type": "string"
    },
    "locketAddress": {
      "type": "string"
    },
    "loggregatorCertificateSecret": {
      "type": "string"
    },
    "nodeSelector": {
      "additionalProperties": true,
      "type": ["object", "null"]
    },
    "resources": {
      "additionalProperties": true,
      "type": ["object", "null"]
    },
    "stacks": {
      "additionalProperties": true,
      "type": "object"
    },
    "tolerations": {
      "type": ["array", "null"],
      "items": {
        "$ref": "https://raw.githubusercontent.com/yannh/kubernetes-json-schema/master/master-standalone-strict/toleration.json"
      }
    },
    "workloadsNamespace": {
      "type": "string"
    }
  },
  "type": "object"
}